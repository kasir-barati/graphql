{
  "name": "botprobe-nest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/botprobe-nest/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "botprobe-nest:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "botprobe-nest:build:development"
        },
        "production": {
          "buildTarget": "botprobe-nest:build:production"
        }
      }
    },
    "prisma-deploy": {
      "executor": "@nx-tools/nx-prisma:deploy"
    },
    "prisma-format": {
      "executor": "@nx-tools/nx-prisma:format"
    },
    "prisma-generate": {
      "executor": "@nx-tools/nx-prisma:generate",
      "dependsOn": ["cleanup"]
    },
    "cleanup": {
      "executor": "nx:run-commands",
      "parallelism": false,
      "cache": false,
      "options": {
        "commands": [
          {
            "command": "rm -rf src/@generated",
            "forwardAllArgs": false
          }
        ],
        "cwd": "{projectRoot}"
      }
    },
    "prisma-migrate": {
      "executor": "@nx-tools/nx-prisma:migrate"
    },
    "prisma-pull": {
      "executor": "@nx-tools/nx-prisma:pull"
    },
    "prisma-push": {
      "executor": "@nx-tools/nx-prisma:push"
    },
    "prisma-reset": {
      "executor": "@nx-tools/nx-prisma:reset"
    },
    "prisma-resolve": {
      "executor": "@nx-tools/nx-prisma:resolve"
    },
    "prisma-seed": {
      "executor": "@nx-tools/nx-prisma:seed"
    },
    "prisma-status": {
      "executor": "@nx-tools/nx-prisma:status"
    },
    "prisma-studio": {
      "executor": "@nx-tools/nx-prisma:studio"
    },
    "prisma-validate": {
      "executor": "@nx-tools/nx-prisma:validate"
    }
  }
}
